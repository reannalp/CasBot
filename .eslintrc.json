{
    "env": {
        "browser": true,
        "es6": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaVersion": 2018,
        "sourceType": "module"
    },
    "rules": {
      "no-console": 0,
      "consistent-return": 0,
      "no-unused-vars": 0,
      "no-dynamic-require": 0,
      "global-require": 0,
      "max-len": 0
    }
}